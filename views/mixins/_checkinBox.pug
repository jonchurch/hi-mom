mixin checkinBox(item)
    .list-group-item 
      .h3.font-weight-bold=item.venue.name
      if item.shout
        .h4.font-italic "#{item.shout}"
      if item.photos.count > 0
        each pic in item.photos.items
          img.img-thumbnail(src=pic.prefix + '100x100' + pic.suffix)
        
      .time-ago #{moment(item.createdAt * 1000).fromNow()}
